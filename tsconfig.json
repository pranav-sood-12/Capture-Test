{
  "compilerOptions": {
      "strict": true,
      "noImplicitAny": true,
      "esModuleInterop": true,
      "strictNullChecks": true,
      "target": "ES2022",
      "module": "CommonJS",
      "moduleResolution": "node",
      "declaration": true,
      "isolatedModules": true,
      "noEmit": true,
      "outDir": "./dist",
      "skipLibCheck": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
